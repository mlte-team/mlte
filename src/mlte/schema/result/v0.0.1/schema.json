{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Result",
    "properties": {
        "schema_version": {
            "type": "string",
            "description": "The version identifier for the schema."
        },
        "metadata": {
            "type": "object",
            "properties": {
                "identifier": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string",
                            "description": "The name that identifies the identifier."
                        }
                    }
                },
                "measurement": {
                    "type": "object",
                    "properties": {
                        "typename": {
                            "type": "string",
                            "description": "The typename identifier for the producing measurement."
                        }
                    }
                },
                "result": {
                    "type": "object",
                    "properties": {
                        "typename": {
                            "type": "string",
                            "description": "The typename identifier for the result itself."
                        }
                    }
                }
            }
        },
        "payload": {
            "type": "object",
            "additionalProperties": true
        }
    },
    "additionalProperties": false,
    "required": [
        "schema_version"
    ]
}