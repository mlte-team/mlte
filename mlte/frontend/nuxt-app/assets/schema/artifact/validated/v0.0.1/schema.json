{
  "$defs": {
    "EvidenceMetadata": {
      "description": "A simple wrapper model for evidence metadata.",
      "properties": {
        "test_case_id": {
          "title": "Test Case Id",
          "type": "string"
        },
        "measurement": {
          "$ref": "#/$defs/MeasurementMetadata"
        }
      },
      "required": [
        "test_case_id",
        "measurement"
      ],
      "title": "EvidenceMetadata",
      "type": "object"
    },
    "MeasurementMetadata": {
      "description": "Info about a Measurement.",
      "properties": {
        "measurement_class": {
          "title": "Measurement Class",
          "type": "string"
        },
        "output_class": {
          "title": "Output Class",
          "type": "string"
        },
        "additional_data": {
          "default": {},
          "title": "Additional Data",
          "type": "object"
        }
      },
      "required": [
        "measurement_class",
        "output_class"
      ],
      "title": "MeasurementMetadata",
      "type": "object"
    },
    "ResultModel": {
      "description": "A description of a Result.",
      "properties": {
        "type": {
          "title": "Type",
          "type": "string"
        },
        "message": {
          "title": "Message",
          "type": "string"
        },
        "evidence_metadata": {
          "anyOf": [
            {
              "$ref": "#/$defs/EvidenceMetadata"
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "required": [
        "type",
        "message",
        "evidence_metadata"
      ],
      "title": "ResultModel",
      "type": "object"
    }
  },
  "description": "The model implementation for the TestResults artifact.",
  "properties": {
    "artifact_type": {
      "const": "validated_spec",
      "default": "validated_spec",
      "title": "Artifact Type",
      "type": "string"
    },
    "test_suite_id": {
      "default": "",
      "title": "Test Suite Id",
      "type": "string"
    },
    "results": {
      "additionalProperties": {
        "$ref": "#/$defs/ResultModel"
      },
      "default": {},
      "title": "Results",
      "type": "object"
    }
  },
  "title": "TestResultsModel",
  "type": "object"
}