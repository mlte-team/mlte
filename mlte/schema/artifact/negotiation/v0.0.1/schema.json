        "access": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Access"
        },
        "labeling_method": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Labeling Method"
        },
        "labels": {
          "default": [],
          "items": {
            "$ref": "#/$defs/LabelDescriptor"
          },
          "title": "Labels",
          "type": "array"
        },
        "fields": {
          "default": [],
          "items": {
            "$ref": "#/$defs/FieldDescriptor"
          },
          "title": "Fields",
          "type": "array"
        },
        "rights": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Rights"
        },
        "policies": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Policies"
