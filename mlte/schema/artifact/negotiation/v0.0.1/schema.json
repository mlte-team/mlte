        "$ref": "#/$defs/DataDescriptor"
      },
      "title": "Data",
      "type": "array"
    },
    "model": {
      "$ref": "#/$defs/ModelDescriptor",
      "default": {
        "development_compute_resources": {
          "cpu": null,
          "gpu": null,
          "gpu_memory": null,
          "main_memory": null,
          "storage": null
        },
        "deployment_platform": null,
        "capability_deployment_mechanism": null,
        "model_source": null,
        "input_specification": [],
        "output_specification": [],
        "production_compute_resources": {
          "cpu": null,
          "gpu": null,
          "gpu_memory": null,
          "main_memory": null,
          "storage": null
