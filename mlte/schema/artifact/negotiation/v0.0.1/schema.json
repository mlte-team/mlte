  "description": "The model implementation for the NegotiationCard artifact.",
  "properties": {
    "artifact_type": {
      "const": "card",
      "default": "card",
      "title": "Artifact Type",
      "type": "string"
    },
    "system": {
      "$ref": "#/$defs/SystemDescriptor",
      "default": {
        "goals": [],
        "problem_type": null,
        "task": null,
        "usage_context": null,
        "risks": []
