        "output_specification": {
          "default": [],
          "items": {
            "$ref": "#/$defs/ModelIODescriptor"
          },
          "title": "Output Specification",
          "type": "array"
        },
        "production_compute_resources": {
          "$ref": "#/$defs/ModelResourcesDescriptor",
          "default": {
            "cpu": null,
            "gpu": null,
            "gpu_memory": null,
            "main_memory": null,
            "storage": null
          }
        }
      },
      "title": "ModelDescriptor",
      "type": "object"
    },
    "ModelIODescriptor": {
      "description": "A description of the model input or output specification.",
      "properties": {
        "name": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Name"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Description"
