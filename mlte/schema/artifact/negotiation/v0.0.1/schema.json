    "ModelDescriptor": {
      "description": "A descriptor for the model.",
      "properties": {
        "development_compute_resources": {
          "$ref": "#/$defs/ModelResourcesDescriptor",
          "default": {
            "cpu": null,
            "gpu": null,
            "gpu_memory": null,
            "main_memory": null,
            "storage": null
          }
        },
        "deployment_platform": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Deployment Platform"
        },
        "capability_deployment_mechanism": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
