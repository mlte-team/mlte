        "expected_values": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Expected Values"
        }
      },
      "title": "ModelIODescriptor",
      "type": "object"
    },
    "ModelResourcesDescriptor": {
      "description": "A descriptor for model resource requirements.",
