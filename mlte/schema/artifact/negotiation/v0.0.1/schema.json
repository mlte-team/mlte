        "classification",
        "clustering",
        "trend",
        "alert",
        "forecasting",
        "content_generation",
        "benchmarking",
        "goals",
        "detection",
        "other"
      ],
      "title": "ProblemType",
      "type": "string"
    },
    "QASDescriptor": {
      "description": "Describes the system-level requirements for the model component. Represents a Quality Attribute Scenario.",
